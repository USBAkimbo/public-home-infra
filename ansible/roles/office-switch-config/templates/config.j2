version 15.0
no service pad
service timestamps debug datetime msec
service timestamps log datetime msec
service password-encryption
!
hostname OFFICE-SW1
!
boot-start-marker
boot-end-marker
!
no logging monitor
!
username admin privilege 15 password 7 {{ PASSWORD }}
no aaa new-model
system mtu routing 1500
!
!
no ip domain-lookup
ip domain-name greenapple.lan
!
port-channel load-balance src-dst-mac
spanning-tree mode pvst
spanning-tree portfast default
spanning-tree portfast bpduguard default
spanning-tree extend system-id
!
vlan internal allocation policy ascending
!
interface Port-channel1
 description Uplink to OFFICE-SW1
 switchport mode trunk
!
interface GigabitEthernet0/1
!
interface GigabitEthernet0/2
!
interface GigabitEthernet0/3
 description TEMP - GUEST
 switchport access vlan 7
 switchport mode access
!
interface GigabitEthernet0/4
 description AP-Office
 switchport trunk native vlan 4
 switchport trunk allowed vlan 4,6,7
 switchport mode trunk
!
interface GigabitEthernet0/5
 description Work laptop
 switchport access vlan 7
 switchport mode access
!
interface GigabitEthernet0/6
 description Garage Uplink
 switchport mode trunk
 channel-group 1 mode on
!
interface GigabitEthernet0/7
 description Garage Uplink
 switchport mode trunk
 channel-group 1 mode on
!
interface GigabitEthernet0/8
 description Conor PC
 switchport access vlan 4
 switchport mode access
!
interface Vlan1
 ip address 192.168.0.252 255.255.255.0
!
interface Vlan4
 ip address 10.10.4.22 255.255.255.0
!
ip default-gateway 10.10.4.254
no ip http server
no ip http secure-server
logging console warnings
logging origin-id hostname
logging 10.10.3.2
snmp-server community public RO
!
no vstack
!
line con 0
 logging synchronous
line vty 0 4
 privilege level 15
 logging synchronous
 login local
 transport input ssh
line vty 5 15
 privilege level 15
 login
!
ntp server 139.162.219.252
end
